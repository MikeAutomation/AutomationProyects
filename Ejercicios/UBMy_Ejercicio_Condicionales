a=30
b=30
c=30
if(a>=b):
    print("El mayor es a: " + str(a))
else:
    print("El mayor es b: "+str(b))


nom = "Miguel"
if(nom=="Miguel"):
    print("Tu nombre es: "+nom)

if(a<=b):
    print("{} es menor o igual que {}". format(a,b))
else:
    print("{} es menor o igual que {}". format(a,b))

if(a<b and a>c):
    print("el mayor es: "+str(a))
elif(b>a and b>c):
    print("el mayor es: "+str(b))
elif(c>a and c>b):
    print("el mayor es: "+str(c))
else:
    print("son iguales todas")